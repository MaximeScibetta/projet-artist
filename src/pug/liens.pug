extends _default.pug

block page-name
    - var pageName = 'liens'
    - var pageNameClass = 'liens'


block content
    ul(class=pageNameClass + '__links-list')
        li(class=pageNameClass + '__links-list__item')
            a(href="#", class=pageNameClass + '__links-list__link', title="visiter le site web Officiel des arts", rel="external")= 'Officiel des arts'
        li(class=pageNameClass + '__links-list__item')
            a(href="#", class=pageNameClass + '__links-list__link', title="visiter le site web Maulpoix", rel="external")= 'Maulpoix'
        li(class=pageNameClass + '__links-list__item')
            a(href="#", class=pageNameClass + '__links-list__link', title="visiter le site web Poster shop", hreflang="en", rel="external")= 'Poster shop'
        li(class=pageNameClass + '__links-list__item')
            a(href="#", class=pageNameClass + '__links-list__link', title="visiter le site web My climats painters", hreflang="en", rel="external")= 'My climats painters'
        li(class=pageNameClass + '__links-list__item')
            a(href="#", class=pageNameClass + '__links-list__link', title="visiter le site web pro wanadoo", rel="external")= 'pro wanadoo'

block endContent
    a(class=pageNameClass + '__cta-to-index cta', href='index.html',  title='Aller vers la page accueil - biographie') Lire sa biographie
