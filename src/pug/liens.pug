extends _default.pug

block page-name
    - var pageName = 'liens'


block content
    ul(class=pageName + '__links-list')
        each val,index in {'Officiel des arts': '', 'Maulpoix': '', 'Poster shop': '', 'My climats painters': '', 'pro wanadoo': ''}
            li(class=pageName + '__links-list__item')
                a(href=val, class=pageName + '__links-list__link')= index